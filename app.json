{
  "expo": {
    "name": "gltapp2",
    "slug": "gltapp2",
    "version": "1.7.5",
    "orientation": "portrait",
    "icon": "./assets/images/Glt_icon.png",
    "scheme": "https",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lvl0-x.gltapp2",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "NSBluetoothAlwaysUsageDescription": "This app uses Bluetooth to connect to printers and scanners for package management.",
        "NSBluetoothPeripheralUsageDescription": "This app uses Bluetooth to connect to printers and scanners for package management.",
        "NSLocationWhenInUseUsageDescription": "This app uses location services to discover nearby Bluetooth devices."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Glt_adaptive.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.lvl0_x.gltapp2",
      "googleServicesFile": "./android/app/google-services.json",
      "permissions": [
        "android.permission.BLUETOOTH",
        "android.permission.BLUETOOTH_ADMIN",
        "android.permission.BLUETOOTH_CONNECT",
        "android.permission.BLUETOOTH_SCAN",
        "android.permission.BLUETOOTH_ADVERTISE",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.CAMERA",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.VIBRATE",
        "android.permission.WAKE_LOCK",
        "android.permission.REQUEST_INSTALL_PACKAGES",
        "android.permission.INSTALL_PACKAGES",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "com.google.android.c2dm.permission.RECEIVE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "notification": {
      "icon": "./assets/images/notification-icon.png",
      "color": "#7c3aed",
      "iosDisplayInForeground": false,
      "androidMode": "default",
      "androidCollapsedTitle": "GLT Logistics Update",
      "sounds": {
        "default": "./assets/sounds/notification_sound.wav",
        "high": "./assets/sounds/notification_high.wav",
        "urgent": "./assets/sounds/notification_urgent.wav"
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Glt_splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store",
      "expo-web-browser",
      "expo-dev-client",
      "react-native-ble-plx",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon.png",
          "color": "#7c3aed",
          "defaultChannel": "default",
          "enableBackgroundRemoteNotifications": true
        }
      ],
      [
        "@react-native-firebase/app",
        {
          "android_package_name": "com.lvl0_x.gltapp2",
          "ios_bundle_id": "com.lvl0-x.gltapp2"
        }
      ],
      [
        "@react-native-firebase/messaging",
        {
          "android_default_channel_id": "default",
          "android_default_notification_color": "#7c3aed",
          "android_default_notification_icon": "@drawable/notification_icon"
        }
      ],
      [
        "react-native-permissions",
        {
          "iosPermissionsArray": [
            "NSBluetoothAlwaysUsageDescription",
            "NSBluetoothPeripheralUsageDescription",
            "NSLocationWhenInUseUsageDescription"
          ]
        }
      ],
      "expo-build-properties",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "EXPO_PUBLIC_API_URL": "https://glt-53x8.onrender.com",
      "expoClientId": "918422141757-hn16hqoqomoofka1l63hcbs47jt4n0o4.apps.googleusercontent.com",
      "webClientId": "918422141757-j819d3oo87i2j6o7bdrebedrtv9nusu5.apps.googleusercontent.com",
      "androidClientId": "918422141757-hn16hqoqomoofka1l63hcbs47jt4n0o4.apps.googleusercontent.com",
      "iosClientId": "918422141757-mj6eei58d2kn90hmsc3ram2vgvl3ck03.apps.googleusercontent.com",
      "router": {},
      "eas": {
        "projectId": "cd8db67e-8126-40ba-9bb0-aaadbb5bbbf7"
      },
      "firebaseConfig": {
        "apiKey": "YOUR_API_KEY",
        "authDomain": "YOUR_PROJECT.firebaseapp.com",
        "projectId": "YOUR_PROJECT_ID",
        "storageBucket": "YOUR_PROJECT.appspot.com",
        "messagingSenderId": "YOUR_SENDER_ID",
        "appId": "YOUR_APP_ID"
      }
    },
    "owner": "lvl0_x"
  }
}