{
  "expo": {
    "name": "gltapp2",
    "slug": "gltapp2",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/Glt_icon.png",
    "scheme": "gltapp2",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lvl0-x.gltapp2"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Glt_adaptive.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.lvl0_x.gltapp2"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Glt_splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "googleClientId": "YOUR_GOOGLE_CLIENT_ID_HERE"
    }
  }
}