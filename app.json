{
  "expo": {
    "name": "gltapp2",
    "slug": "gltapp2",
    "version": "1.6.0",
    "orientation": "portrait",
    "icon": "./assets/images/Glt_icon.png",
    "scheme": "https",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lvl0-x.gltapp2",
      "infoPlist": {
        "NSBluetoothAlwaysUsageDescription": "This app uses Bluetooth to connect to printers and scanners for package management.",
        "NSBluetoothPeripheralUsageDescription": "This app uses Bluetooth to connect to printers and scanners for package management.",
        "NSLocationWhenInUseUsageDescription": "This app uses location services to discover nearby Bluetooth devices."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Glt_adaptive.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.lvl0_x.gltapp2",
      "permissions": [
        "android.permission.BLUETOOTH",
        "android.permission.BLUETOOTH_ADMIN",
        "android.permission.BLUETOOTH_CONNECT",
        "android.permission.BLUETOOTH_SCAN",
        "android.permission.BLUETOOTH_ADVERTISE",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.CAMERA",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.VIBRATE",
        "android.permission.WAKE_LOCK",
        "android.permission.REQUEST_INSTALL_PACKAGES",
        "android.permission.INSTALL_PACKAGES"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Glt_splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store",
      "expo-web-browser",
      "expo-dev-client",
      "react-native-ble-plx",
      [
        "react-native-permissions",
        {
          "iosPermissionsArray": [
            "NSBluetoothAlwaysUsageDescription",
            "NSBluetoothPeripheralUsageDescription", 
            "NSLocationWhenInUseUsageDescription"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "EXPO_PUBLIC_API_URL": "https://glt-53x8.onrender.com",
      "expoClientId": "918422141757-hn16hqoqomoofka1l63hcbs47jt4n0o4.apps.googleusercontent.com",
      "webClientId": "918422141757-j819d3oo87i2j6o7bdrebedrtv9nusu5.apps.googleusercontent.com",
      "androidClientId": "918422141757-hn16hqoqomoofka1l63hcbs47jt4n0o4.apps.googleusercontent.com",
      "iosClientId": "918422141757-mj6eei58d2kn90hmsc3ram2vgvl3ck03.apps.googleusercontent.com",
      "router": {},
      "eas": {
        "projectId": "cd8db67e-8126-40ba-9bb0-aaadbb5bbbf7"
      }
    },
    "owner": "lvl0_x"
  }
}